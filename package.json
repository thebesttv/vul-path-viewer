{
  "name": "thebesttv-path-viewer",
  "displayName": "Path Viewer",
  "description": "View path generated by tool",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "path-viewer",
          "title": "Path Viewer",
          "icon": "media/path.svg"
        }
      ]
    },
    "views": {
      "path-viewer": [
        {
          "id": "all-paths",
          "name": "All Paths",
          "icon": "media/path.svg",
          "contextualTitle": "Path Viewer"
        },
        {
          "id": "path-details",
          "name": "Path Details",
          "icon": "media/path.svg",
          "contextualTitle": "Path Viewer"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "all-paths",
        "contents": "No output.json chosen yet"
      },
      {
        "view": "path-details",
        "contents": "No path chosen yet"
      }
    ],
    "commands": [
      {
        "command": "thebesttv-path-viewer.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
